		{
			"name": "get weekly report",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Response status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"",
							"pm.test(\"Response has the required fields - Confirmed, Rejected, and Pending\", function () {",
							"    const responseData = pm.response.json();",
							"    ",
							"    pm.expect(responseData).to.be.an('object');",
							"    pm.expect(responseData).to.have.property('Confirmed');",
							"    pm.expect(responseData).to.have.property('Rejected');",
							"    pm.expect(responseData).to.have.property('Pending');",
							"});",
							"",
							"",
							"pm.test(\"Confirmed, Rejected, and Pending are non-negative integers\", function () {",
							"  const responseData = pm.response.json();",
							"  ",
							"  pm.expect(responseData).to.be.an('object');",
							"  pm.expect(responseData.Confirmed).to.be.a('number').and.to.be.at.least(0);",
							"  pm.expect(responseData.Rejected).to.be.a('number').and.to.be.at.least(0);",
							"  pm.expect(responseData.Pending).to.be.a('number').and.to.be.at.least(0);",
							"});",
							""
						],
						"type": "text/javascript",
						"packages": {}
